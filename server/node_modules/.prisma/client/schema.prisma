generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:Alejor-4278@localhost:3306/newDB"
}

model User {
  userID  Int     @id @default(autoincrement())
  username String? @db.VarChar(35)
  password String? @db.VarChar(30)
}

model Stockwerke {
  stockwerkID   Int  @default(autoincrement())   @id
  stockwerkNr   Int?
  stockwerkName String? @db.VarChar(30)
}

model Sitzungszimmer {
  sitzungszimmerID Int @default(autoincrement()) @id
  anzPersonen Int @unique
  sitzungsZimmerStandort Standorte @relation(fields: [sitzungszimmerStID], references: [standortID])
  sitzungszimmerStID Int
}

model Standorte {
  standortID Int @default(autoincrement())   @id
  standortName String @unique
  sitzungsZimmer Sitzungszimmer[]
}

model anzUser {
  anzUser Int
  anzUserId Int @default(autoincrement()) @id
}
